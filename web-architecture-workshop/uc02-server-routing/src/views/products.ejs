<!DOCTYPE html>
<html>
<head>
    <title>Online Store - <%= title %></title>
    <style>
        body {
            font-family: Arial, sans-serif;
            margin: 0;
            padding: 20px;
        }
        nav {
            background-color: #333;
            padding: 15px;
            margin-bottom: 20px;
        }
        nav a {
            color: white;
            text-decoration: none;
            padding: 10px 15px;
            margin-right: 10px;
        }
        nav a:hover {
            background-color: #555;
        }
        .product {
            border: 1px solid #ddd;
            padding: 15px;
            margin: 10px;
            display: inline-block;
            width: 200px;
        }
        .button {
            background-color: #4CAF50;
            color: white;
            padding: 10px 15px;
            border: none;
            cursor: pointer;
        }
    </style>
</head>
<body>
    <nav>
        <a href="/">Home</a>
        <a href="/products">Products</a>
        <a href="/about">About</a>
    </nav>
    <h1>Our Products</h1>
    <% products.forEach(product => { %>
    <div class="product">
        <h2><%= product.name %></h2>
        <p>Price: <%= product.price %></p>
        <form action="/cart">
            <button type="submit" class="button">Add to Cart</button>
        </form>
    </div>
    <% }) %>
</body>
</html>